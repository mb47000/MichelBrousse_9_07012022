      "!**/fixtures/**",
      "!**/lcov-report/**"
    ]
  },
  "dependencies": {
    "express": "^4.17.1",
    "global": "^4.4.0",
    "jquery": "^3.5.1",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.10.4",
    "@testing-library/dom": "^7.20.0",
    "@testing-library/jest-dom": "^5.11.0",
    "@testing-library/user-event": "^12.0.11",
    "babel-jest": "^26.1.0",
    "jest": "^26.1.0",
    "jest-html-reporter": "^3.1.3",
    "msw": "^0.36.3",
    "whatwg-fetch": "^3.6.2",
    "chromedriver": "^97.0.0",
    "geckodriver": "^2.0.0",
    "nightwatch": "^1.7.6"
